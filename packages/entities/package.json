{
  "name": "@scalar/entities",
  "version": "0.0.3",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "scalar-build-rollup",
    "dev": "vite",
    "format:check": "prettier --check --ignore-path=../../.prettierignore .",
    "format:write": "prettier --write --ignore-path=../../.prettierignore .",
    "lint:check": "eslint .",
    "lint:fix": "eslint .  --fix",
    "preview": "vite preview",
    "test:unit": "vitest src",
    "types:build": "scalar-types-build",
    "types:check": "scalar-types-check"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utility": {
      "import": "./dist/utility/index.js",
      "types": "./dist/utility/index.d.ts"
    },
    "./user": {
      "import": "./dist/user/index.js",
      "types": "./dist/user/index.d.ts"
    },
    "./token": {
      "import": "./dist/token/index.js",
      "types": "./dist/token/index.d.ts"
    },
    "./theme": {
      "import": "./dist/theme/index.js",
      "types": "./dist/theme/index.d.ts"
    },
    "./team": {
      "import": "./dist/team/index.js",
      "types": "./dist/team/index.d.ts"
    },
    "./publish": {
      "import": "./dist/publish/index.js",
      "types": "./dist/publish/index.d.ts"
    },
    "./project": {
      "import": "./dist/project/index.js",
      "types": "./dist/project/index.d.ts"
    },
    "./helios": {
      "import": "./dist/helios/index.js",
      "types": "./dist/helios/index.d.ts"
    },
    "./deploys": {
      "import": "./dist/deploys/index.js",
      "types": "./dist/deploys/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "nanoid": "^5",
    "zod": "^3"
  },
  "devDependencies": {
    "@scalar/build-tooling": "*",
    "vite": "5.3.5",
    "vitest": "2.0.4"
  }
}
